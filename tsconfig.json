{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES6",
    "moduleResolution": "classic",
    "lib": [
      "ES2019",
      "dom"
    ],
    "allowJs": true,
    "checkJs": true,
    "noEmit": false,
    "strict": true,
    "strictPropertyInitialization": false,
    "emitDeclarationOnly": true,
    "declaration": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "esModuleInterop": true
  },
  "include": [
    "src/brokers/**/*.js"
  ]
}
